{
  "name": "directus-extension-rokka-interface",
  "version": "0.1.0",
  "author": {
    "email": "juerg.hunziker@liip.ch",
    "name": "Juerg Hunziker"
  },
  "license": "gpl-3.0",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-interface",
    "rokka"
  ],
  "directus:extension": {
    "type": "interface",
    "path": "dist/index.js",
    "source": "src/index.ts",
    "host": "^9.9.1"
  },
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "lint": "eslint --ext .js,.ts,.vue src/",
    "format": "eslint --ext .js,.ts,.vue --fix src/"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@directus/extensions-sdk": "^9.15.1",
    "directus-codestyle": "dimitrov-adrian/directus-codestyle",
    "sass": "^1.54.4",
    "typescript": "^4.7.4"
  },
  "prettier": "directus-codestyle/prettier",
  "stylelint": {
    "extends": "directus-codestyle/stylelint"
  },
  "eslintConfig": {
    "extends": "./node_modules/directus-codestyle/.eslintrc.js",
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
